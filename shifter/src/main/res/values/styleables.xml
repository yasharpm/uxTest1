<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleMenu">
        <attr name="radiusRatio" format="float"/>
        <attr name="drawCircle" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ServiceView">
        <attr name="name" format="string"/>
        <attr name="color" format="color"/>
        <attr name="isRed" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="GigilView">
        <attr name="rtl" format="boolean"/>
        <attr name="color"/>
        <attr name="icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SideMenu">
        <attr name="layout" format="reference"/>
        <attr name="rtl"/>
    </declare-styleable>

    <declare-styleable name="ServiceItem">
        <attr name="icon"/>
        <attr name="name"/>
    </declare-styleable>

</resources>